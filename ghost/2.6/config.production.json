{
    "url": "http://[[domainName]]",
    "server": {
        "host": "0.0.0.0",
        "port": 9000
    },
    "database": {
        "client": "mysql",
        "connection": {
            "host"     : "[[host]]",
            "user"     : "[[user]]",
            "password" : "[[password]]",
            "database" : "[[database]]"
        }
    },
    "paths": {
        "contentPath": "content/"
    },
    "logging": {
        "level": "info",
        "rotation": {
            "enabled": true
        },
        "transports": ["file", "stdout"]
    }
}
